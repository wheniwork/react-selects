{
  "name": "react-select-plus",
  "version": "1.0.0-rc.3.patch4",
  "description": "A fork of react-select with support for option groups",
  "main": "lib/Select.js",
  "style": "dist/react-select-plus.min.css",
  "author": "When I Work",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wheniwork/react-selects.git"
  },
  "jest": {
    "modulePaths": [
      "src"
    ],
    "coverageDirectory": "<rootDir>/reports/coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "classnames": "^2.2.4",
    "prop-types": "^15.5.8",
    "react-input-autosize": "^1.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.11.12",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "gulp": "^3.9.1",
    "isomorphic-fetch": "^2.2.1",
    "istanbul": "^0.4.5",
    "jsdom": "^10.0.0",
    "mocha": "^3.0.2",
    "postcss": "^5.2.17",
    "react": "^15.0",
    "react-addons-shallow-compare": "^15.0",
    "react-addons-test-utils": "^15.0",
    "react-component-gulp-tasks": "^0.7.7",
    "react-dom": "^15.0",
    "react-gravatar": "^2.4.5",
    "react-highlight-words": "^0.3.0",
    "react-virtualized": "^7.23.0",
    "react-virtualized-select": "^1.4.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-sass": "^0.5.1",
    "sinon": "^2.1.0",
    "unexpected": "^10.17.0",
    "unexpected-dom": "^3.1.0",
    "unexpected-react": "^3.2.4",
    "unexpected-sinon": "^10.4.0"
  },
  "peerDependencies": {
    "react": "^0.14 || ^15.0.0-rc || ^15.0",
    "react-dom": "^0.14 || ^15.0.0-rc || ^15.0"
  },
  "scripts": {
    "build": "rollup -c",
    "cover": "istanbul cover _mocha -- -u exports --compilers js:babel/register -R spec",
    "coveralls": "NODE_ENV=test istanbul cover _mocha --report lcovonly -- -u exports --compilers js:babel-register -R spec && cat coverage/lcov.info | coveralls",
    "lint": "eslint .",
    "test": " mocha --compilers js:babel-register"
  },
  "keywords": [
    "combobox",
    "form",
    "input",
    "multiselect",
    "react",
    "react-component",
    "select",
    "ui"
  ]
}
